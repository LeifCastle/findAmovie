<link rel="stylesheet" href="/css/profile.css" />
<h3>Hello, <%=firstName %></h3>

<div>
  <p>First Name: <%= firstName %></p>
  <p>Last Name: <%= lastName %></p>
  <p>User Name: <%= username %></p>
</div>

<h2>Here are the movie's you've seen</h2>
<div id="seenMovies">
  <% seenMovies.forEach(movie => { %>
  <div class="seenMovieCard">
    <p><%= movie.title %></p>
    <img class="moviePoster" src="<%=movie.posterImage%>" alt="posterImage" />
  </div>
  <% }) %>
</div>
